        <Unclipped val="true" />
        <Width val="0" />
        <Height val="0" />
        <StateGroup name="PlayOnceAnimationState">
          <DefaultState val="IntroOff" />
          <State name="IntroOff">
            <When type="AnimationState" frame="$this" PlayOnceAnimationState="IntroOff" />
            <Action type="SendEvent" frame="$this" event="StopAll" />
          </State>
          <State name="IntroOn">
            <When type="AnimationState" frame="$this" PlayOnceAnimationState="IntroOn" />
            <Action type="SendEvent" frame="$this" event="PlayIntro" />
          </State>
        </StateGroup>
        <Animation name="SpawnInPlayOnceAnimation">
          <Event event="PlayIntro" action="Reset, Play" />
          <Event event="StopAll" action="Reset, Stop" />
          <Controller type="Anchor" end="Pause" unclipped="True" side="Left" relative="true">
            <Key type="Curve" time="0.0" value="-92" inout="Linear" />
            <Key type="Curve" time="0.7" value="0" inout="Smooth,Linear" />
          </Controller>
          <Controller type="Rotation" end="Pause" relative="false" frame="GameButtonDefaultBorder">
            <Key type="Curve" time="0.0" value="-90" inout="Linear" />
            <Key type="Curve" time="0.7" value="0" inout="Smooth,Linear" />
          </Controller>
          <Controller type="Rotation" end="Pause" relative="false" frame="IconBackground">
            <Key type="Curve" time="0.0" value="-90" inout="Linear" />
            <Key type="Curve" time="0.7" value="0" inout="Smooth,Linear" />
          </Controller>
          <Controller type="Rotation" end="Pause" relative="false" frame="DisabledLockOverlay">
            <Key type="Curve" time="0.0" value="-90" inout="Linear" />
            <Key type="Curve" time="0.7" value="0" inout="Smooth,Linear" />
          </Controller>
          <Controller type="Rotation" end="Pause" relative="false" frame="NormalImage">
            <Key type="Curve" time="0.0" value="-90" inout="Linear" />
            <Key type="Curve" time="0.7" value="0" inout="Smooth,Linear" />
          </Controller>
          <Controller type="Fade" end="Pause">
            <Key type="Curve" time="0.0" value="0" inout="Linear,Step" />
            <Key type="Curve" time="0.2" value="0" inout="Step,Smooth" />
            <Key type="Curve" time="0.6" value="255" inout="Smooth,Linear" />
          </Controller>
          <Controller type="Animation" end="stop" frame="$FKeyButtonPulse1" Animation="ButtonPulse">
            <Key type="Animation" time="0" action="reset" />
            <Key type="Animation" time="0.7" action="play" />
          </Controller>
          <Controller type="Animation" end="stop" frame="$FKeyButtonPulse2" Animation="ButtonPulse">
            <Key type="Animation" time="0" action="reset" />
            <Key type="Animation" time="0.9" action="play" />
          </Controller>
          <Controller type="Animation" end="stop" frame="$FKeyButtonPulse3" Animation="ButtonPulse">
            <Key type="Animation" time="0" action="reset" />
            <Key type="Animation" time="1.1" action="play" />
          </Controller>
        </Animation>
