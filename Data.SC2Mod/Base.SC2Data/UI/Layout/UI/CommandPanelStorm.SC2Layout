  <Animation name="FlipCommandPanel">
    <Event event="FlipCommandPanel" action="Restart, Play" />
    <Controller type="Anchor" end="Pause" side="Bottom" relative="false" frame="AnchorFrame">
      <Key type="Curve" time="0" value="100" inout="Smooth" />
      <Key type="Curve" time=".25" value="0" inout="Smooth" />
    </Controller>
  </Animation>
  <Frame type="Image" name="ButtonPulseTemplate">
    <Anchor side="Top" relative="$parent/GameButtonDefaultBorder" pos="Mid" offset="0" />
    <Anchor side="Left" relative="$parent/GameButtonDefaultBorder" pos="Mid" offset="0" />
    <Anchor side="Right" relative="$parent/GameButtonDefaultBorder" pos="Mid" offset="0" />
    <Anchor side="Bottom" relative="$parent/GameButtonDefaultBorder" pos="Mid" offset="0" />
    <Texture val="@UI/Storm_UI_InGame_AbilityButton_Activate_Add" />
    <BlendMode val="Add" />
    <ColorAdjustMode val="None" />
    <Color val="00c8ff" />
    <Alpha val="0" />
    <Visible val="False" />
    <Unclipped val="True" />
    <Animation name="ButtonPulse">
      <Event event="OnShown" action="Stop" />
      <Event event="ButtonPulse" action="Restart, play" frame="$parent" />
      <Controller type="Dimension" dimension="Height" end="Pause" unclipped="True" percentage="false" relative="False">
        <Key type="Curve" time="0" value="300" inout="Linear" />
        <Key type="Curve" time="0.6" value="116" inout="Linear,Slow" />
        <Key type="Curve" time="0.8" value="116" inout="Slow,Linear" />
      </Controller>
      <Controller type="Dimension" dimension="Width" end="Pause" unclipped="True" percentage="false" relative="False">
        <Key type="Curve" time="0" value="300" inout="Linear" />
        <Key type="Curve" time="0.6" value="116" inout="Linear,Slow" />
        <Key type="Curve" time="0.8" value="116" inout="Slow,Linear" />
      </Controller>
      <Controller type="Fade" end="Pause">
        <Key type="Curve" time="0" value="0" />
        <Key type="Curve" time=".6" value="255" inout="Linear" />
        <Key type="Curve" time="0.8" value="0" inout="Slow,Linear" />
      </Controller>
      <Controller type="Visibility" end="Pause">
        <Key type="Bool" time="1" value="False" />
      </Controller>
    </Animation>
  </Frame>

