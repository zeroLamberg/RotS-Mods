      <Animation name="ToggleVisibility">
        <Event event="Toggled" action="DirectionForward,Play" frame="$HeroLeaderBoardToggleButtonController" />
        <Event event="Normal" action="DirectionReverse,Play" frame="$HeroLeaderBoardToggleButtonController" />
        <Controller type="Fade" end="Pause">
          <Key type="Curve" time="0" value="0" />
          <Key type="Curve" time=".1" value="255" />
        </Controller>
        <Controller type="Anchor" end="Pause" side="Bottom" relative="true">
          <Key type="Curve" time="0" value="-15" inout="Linear" />
          <Key type="Curve" time=".1" value="0" inout="Slow,Linear" />
        </Controller>
        <Controller type="Anchor" end="Pause" side="Top" relative="true">
          <Key type="Curve" time="0" value="-15" inout="Linear" />
          <Key type="Curve" time=".1" value="0" inout="Slow,Linear" />
        </Controller>
        <Controller type="Sound" end="Pause">
          <Key type="Sound" time="0.0" sound="@UI_Game_StatsContainerAppears" direction="Forward" />
          <Key type="Sound" time="0.175" sound="@UI_Game_StatsContainerHides" direction="Reverse" />
        </Controller>
      </Animation>
      
