          <Action type="SetProperty" frame="TeamOrderStats/TeamLevel/Label" Color="{$GameUI/AllyColors/AdditionalColor0/@Color}"/>
          <Action type="SetProperty" frame="TeamOrderStats/TeamTitle" Text="@UI/ScreenScore/Team1LevelTitle" />
          <!-- Team Chaos Stats-->
