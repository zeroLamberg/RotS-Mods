    <Frame type="CommandPanel" name="CommandPanel" template="PlayerAnnouncer/CommandPanelTemplate">
      <!--<Anchor side="Top" relative="$parent/Output" pos="Max" offset="0"/>
      <Anchor side="Left" relative="$parent/Output" pos="Min" offset="0"/>-->
      <Anchor relative="$parent" pos="Mid"/>
      <Width val="250"/>
      <Height val="250"/>
      <Visible val="True"/>

      <Animation name="PausedAnimation">
        <Event event="OnShown" action="Reset,Play" frame="$GameUI/UIContainer/FullscreenLowerContainer/PausePanel"/>
        <Controller type="Enabled" end="Pause" frame="$this">
          <Key type="Bool" time="0.001" value="True"/>
        </Controller>
      </Animation>

      <Frame type="CommandButton" name="CommandButton00">
        <HotkeyUse val="HeroLeaderPanel"/>
        <Animation name="SendEvent">
          <Event event="Pushed" action="Reset,Play"/>
          <Controller type="Event" end="Pause" frame="$HeroLeaderPanel/ToggleButton">
            <Key type="Event" time="0.0" event="ControlClickRequest"/>
          </Controller>
        </Animation>
      </Frame>

      <Frame type="CommandButton" name="CommandButton01">
        <HotkeyUse val="VideoRecord"/>
        <!--<UseAlternateTime val="True"/>-->

        <Animation name="SendEvent">
          <Event event="Pushed" action="Reset,Play"/>
          <Controller type="State" end="Pause" frame="$this" stateGroup="AnnouncerPressed">
            <Key type="Identifier" time="0" value="Pressed"/>
            <Key type="Identifier" time="0.0625" value="Default"/>
          </Controller>
        </Animation>

        <StateGroup name="AvailableState">
          <State name="Inactive">
            <When type="StateGroup" frame="$Announcer" operator="Equal" ContextState="None"/>
            <Action type="SetProperty" frame="$this" Ignored="True"/>
          </State>
          <State name="Default"/>
        </StateGroup>

        <StateGroup name="AnnouncerPressed">
          <DefaultState val="Default"/>
          <State name="Pressed"/>
          <State name="Default"/>
        </StateGroup>

        <StateGroup name="AnnouncerPressedStates">
          <DefaultState val="Default"/>
          <State name="Pressed_Health">
            <When type="StateGroup" frame="$Announcer" HealthBarState="True" />
            <When type="StateGroup" frame="$this" AnnouncerPressed="Pressed" />
            <Action type="SetProperty" frame="$Announcer/Output" Text="{$Announcer/TF_Final_UnitStatus/@Text}" undo="False"/>
          </State>
          <State name="Pressed_Energy">
            <When type="StateGroup" frame="$Announcer" EnergyBarState="True" />
            <When type="StateGroup" frame="$this" AnnouncerPressed="Pressed" />
            <Action type="SetProperty" frame="$Announcer/Output" Text="{$Announcer/TF_Final_UnitStatus/@Text}" undo="False"/>
          </State>
          <State name="Pressed_HealingFountain">
            <When type="StateGroup" frame="$Announcer" HealingFountainState="True" />
            <When type="StateGroup" frame="$this" AnnouncerPressed="Pressed" />
            <Action type="SetProperty" frame="$Announcer/Output" Text="{$Announcer/TF_Final_UnitStatus/@Text}" undo="False"/>
          </State>
          <State name="Pressed_XPContribution">
            <When type="StateGroup" frame="$Announcer" XPContributionState="True" />
            <When type="StateGroup" frame="$this" AnnouncerPressed="Pressed" />
            <Action type="SetProperty" frame="$Announcer/Output" Text="{$Announcer/TopBarFrame/@Text}" undo="False"/>
          </State>
          <State name="Pressed_PartyFrame">
            <When type="StateGroup" frame="$Announcer" PartyFrameState="True" />
            <When type="StateGroup" frame="$this" AnnouncerPressed="Pressed" />
            <Action type="SetProperty" frame="$Announcer/Output" Text="{$Announcer/TF_Final_TopBar/@Text}" undo="False"/>
          </State>
          <State name="Pressed">
            <When type="StateGroup" frame="$this" AnnouncerPressed="Pressed" />
            <Action type="SetProperty" frame="$Announcer/Output" Text="{$Announcer/TF_Buffer/@Text}" undo="False"/>
          </State>
          <State name="Default"/>
        </StateGroup>

      </Frame>

      <Frame type="CommandButton" name="CommandButton02">
        <HotkeyUse val="QuickHeroTalentTreeSelection"/>
        <Animation name="SendEvent">
          <Event event="Pushed" action="Reset,Play"/>
          <Controller type="Event" end="Pause" frame="$GameUI_HeroTalentTreeSelectionToggleButton">
            <Key type="Event" time="0.0" event="ControlClickRequest"/>
          </Controller>
        </Animation>
      </Frame>

    </Frame>
  </Frame>
</Frame>
