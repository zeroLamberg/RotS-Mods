<Frame type="GameUI" name="GameUI" file="GameUI">
  <Frame type="Frame" name="Announcer">
    <Width val="1"/>
    <Height val="1"/>
    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
    <Anchor side="Right" relative="$parent" pos="Min" offset="0"/>
    <!--<Anchor relative="$parent"/>-->
    <Handle val="Announcer"/>
    <Frame type="EditBox" name="Output">
      <Visible val="False"/>
      <Width val="250"/>
      <Height val="150"/>
      <UseAlternateTime val="True"/>
      <Insets top="8" left="8" bottom="8" right="8" />
      <HighlightColor val="#StandardEditBoxHighlightColor" />
      <HighlightAlpha val="#StandardEditBoxHighlightAlpha" />
      <MultiLine val="True"/>
      <Frame type="Image" name="Image">
        <Anchor relative="$parent" />
        <Texture val="@UI/Storm_inputbox" />
        <TextureType val="Nineslice" />
      </Frame>
      <Frame type="Frame" name="LabelContainerFrame">
        <Anchor relative="$parent" />
        <Frame type="Label" name="Label">
          <Anchor relative="$parent" />
          <Style val="StandardEditBoxLabel" />
          <Options val="" />
        </Frame>
      </Frame>
      <StateGroup name="IsEmpty">
        <DefaultState val="Empty"/>
        <State name="NotEmpty">
          <When type="Property" frame="$this" operator="NotEqual" Text=""/>
          <Action type="SendEvent" frame="$this" event="Clear"/>
        </State>
        <State name="Empty"/>
      </StateGroup>
      <Animation name="Clear">
        <Event event="Clear" action="Restart,Play" frame="$this"/>
        <Controller type="Property" end="Pause" frame="$this" property="Text">
          <Key type="Property" time="0.0625" value=""/>
        </Controller>
      </Animation>
    </Frame>

    <Frame type="Label" name="UnitLink">
      <Visible val="False"/>
      <!--<Text val="{$GameUI/UnitFrame/@UnitLink}"/>-->
    </Frame>
    <Frame type="Label" name="UnitTag">
      <Visible val="False"/>
      <!--<Text val="{$GameUI/UnitFrame/@UnitTag}"/>-->
    </Frame>
    <Frame type="Label" name="VitalType">
      <Visible val="False"/>
      <Text val=""/>
    </Frame>
    <Frame type="Label" name="TopBarFrame">
      <Visible val="False"/>
      <Text val=""/>
    </Frame>
    <Frame type="Label" name="TopBarTeam">
      <Visible val="False"/>
      <Text val=""/>
    </Frame>
    <Frame type="Label" name="TopBarPlayerSlot">
      <Visible val="False"/>
      <Text val=""/>
    </Frame>
    <Frame type="Label" name="TopBarHeroIndex">
      <Visible val="False"/>
      <Text val=""/>
    </Frame>

    <Frame type="TextFormatFrame" name="TF_Buffer">
      <FormatText val="%0%&#xA;%1%"/>
      <RequiredReplacements val="2"/>
      <Replacement index="0" val=""/>
      <Replacement index="1" val="{$parent/TF_Final/@Text}"/>
    </Frame>

    <Frame type="TextFormatFrame" name="TF_Button">
      <FormatText val=""/>
    </Frame>
    <Frame type="TextFormatFrame" name="TF_Texture">
      <FormatText val=""/>
    </Frame>
    <Frame type="TextFormatFrame" name="TF_Final">
      <FormatText val="%0%&#xA;%1%&#xA;%2%&#xA;%3%"/>
      <Replacement index="0" val="{$parent/UnitTag/@Text}"/>
      <Replacement index="1" val="{$parent/UnitLink/@Text}"/>
      <Replacement index="2" val="{$parent/TF_Button/@Text}"/>
      <Replacement index="3" val="{$parent/TF_Texture/@Text}"/>
    </Frame>

    <Frame type="TextFormatFrame" name="TF_Final_UnitStatus">
      <FormatText val="%0%&#xA;%1%&#xA;%2%"/>
      <Replacement index="0" val="{$parent/VitalType/@Text}"/>
      <Replacement index="1" val="{$parent/UnitTag/@Text}"/>
      <Replacement index="2" val="{$parent/UnitLink/@Text}"/>
    </Frame>

    <Frame type="TextFormatFrame" name="TF_Final_TopBar">
      <FormatText val="%0%&#xA;%1%&#xA;%2%&#xA;%3%"/>
      <Replacement index="0" val="{$parent/TopBarFrame/@Text}"/>
      <Replacement index="1" val="{$parent/TopBarTeam/@Text}"/>
      <Replacement index="2" val="{$parent/TopBarPlayerSlot/@Text}"/>
      <Replacement index="3" val="{$parent/TopBarHeroIndex/@Text}"/>
    </Frame>

