    <CActorUnit id="MercDefenderSiegeGiant" parent="StormMinionStandard" unitName="MercDefenderSiegeGiant">
        <Aliases value="_MercDefenderSiegeGiant"/>
        <On Terms="ActorCreation" Send="Create SiegeGiantBirthFX"/>
        <On Terms="ActorCreation" Send="Create MercDefenderSiegeGiantBirthSound"/>
        <On Terms="ActorCreation" Send="StatusSet AnimationVariation 0"/>
        <On Terms="WeaponStart.MercDefenderSiegeGiant.AttackStart" Send="AnimPlay Attack Attack"/>
        <On Terms="Effect.MercDefenderSiegeModeMeleeAnimDummy.Start" Send="AnimPlay Attack Attack,Inferior"/>
        <!-- Birth Animation -->
        <On Terms="ActorCreation" Send="$Birth DontResetOnUnhide 0.000000 -1.000000 1.000000 AsTimeScale"/>
        <On Terms="ActorCreation" Send="AnimBracketStart Cover IGNORE Stand,Cover Stand,Cover,End"/>
        <!-- Post-Birth Animation -->
        <On Terms="AnimDone; AnimName Birth; TextureGroupsGlobal ContainsAll luxoria" Send="AnimBracketStart Cover IGNORE Stand,Cover Stand,Cover,End"/>
        <On Terms="AnimDone; AnimName Birth; !TextureGroupsGlobal ContainsAll luxoria" Send="AnimPlay BirthEnd Stand,Cover,Taunt,End 0 -1.000000 -1.000000 1.000000 AsTimeScale"/>
        <!-- Sit Down/Stand Up AnimBracket -->
        <On Terms="Behavior.JungleIdleState.On" Send="AnimBracketStart Cover Stand,Cover,Start Stand,Cover Stand,Cover,End"/>
        <On Terms="Behavior.JungleIdleState.Off" Send="AnimBracketStop Cover"/>
        <!--<On Terms="Behavior.JungleCreepSoftLeashing.Off" Send="AnimBracketStart Cover Stand,Cover,Start Stand,Cover Stand,Cover,End"/>
        <On Terms="Behavior.JungleCreepHardLeashing.Off" Send="AnimBracketStart Cover Stand,Cover,Start Stand,Cover Stand,Cover,End"/>-->
        <!-- Stand Up -->
        <!--<On Terms="Behavior.JungleCreepPrepAggro.On" Send="AnimBracketStop Cover"/>-->
        <On Terms="UnitDeath" Send="AnimBracketStart Depart Stand,Unpowered,Start Stand,Unpowered Stand,Unpowered,End 0 1.000000 AsTimeScale"/>
        <!-- Something was reset, clear any previous Variation matchers -->
        <On Terms="ActorCreation" Send="Create VariationMonitorCleanupDoodad"/>
        <!-- Variation matchers -->
        <On Terms="UnitDeathCustomize; ModelVariation 0" Send="Create ModelVariationStorer0"/>
        <On Terms="UnitDeathCustomize; ModelVariation 1" Send="Create ModelVariationStorer1"/>
        <On Terms="UnitDeathCustomize; ModelVariation 2" Send="Create ModelVariationStorer2"/>
        <On Terms="UnitDeathCustomize; ModelVariation 3" Send="Create ModelVariationStorer3"/>
        <On Terms="UnitDeathCustomize; IsStatus AnimationVariation 1" Send="Create ModelAnimationVariationStorer1"/>
        <On Terms="Behavior.JungleCreepSoftLeashing.On" Send="StatusIncrement Resetting"/>
        <On Terms="Behavior.JungleCreepHardLeashing.On" Send="StatusIncrement Resetting"/>
        <On Terms="Behavior.JungleCreepSoftLeashing.Off" Send="TimerSet 3.500000 Resetting"/>
        <On Terms="Behavior.JungleCreepHardLeashing.Off" Send="TimerSet 3.500000 Resetting"/>
        <On Terms="TimerExpired; TimerName Resetting" Send="StatusSet Resetting 0"/>
        <On Terms="Effect.MercDefenderSiegeGiantTauntDummy.Start" Send="AnimClear Fidget"/>
        <On Terms="Effect.MercDefenderSiegeGiantTauntDummy.Start; !AnimPlaying Birth Birth" Send="AnimPlay TauntStart Stand,Cover,Taunt,Start DontResetOnUnhide -1.000000 -1.000000 1.000000 AsTimeScale"/>
        <On Terms="AnimDone.*.Complete; AnimName TauntStart; ValidateUnit SourceHasJunglePassive" Send="AnimPlay Taunt Stand,Cover,Taunt DontResetOnUnhide -1.000000 -1.000000 1.000000 AsTimeScale"/>
        <On Terms="AnimDone.*.Complete; AnimName Taunt; ValidateUnit SourceHasJunglePassive" Send="AnimPlay TauntEnd Stand,Cover,Taunt,End DontResetOnUnhide -1.000000 -1.000000 1.000000 AsTimeScale"/>
        <On Terms="Behavior.JungleCreepPassive.Off" Send="AnimClear TauntStart"/>
        <On Terms="Behavior.JungleCreepPassive.Off" Send="AnimClear Taunt"/>
        <On Terms="Behavior.JungleCreepPassive.Off" Send="AnimClear TauntEnd"/>
        <On Terms="Behavior.JungleCreepPassive.Off" Send="AnimClear Fidget"/>
        <On Terms="Behavior.JungleCreepPassive.Off" Send="AnimClear BirthEnd"/>
        <Remove Terms="WeaponStart.*.AttackStart" Send="AnimPlay Attack Attack"/>
        <DeathArray index="Normal">
            <AnimProps value="Unpowered"/>
            <ModelLink value="MercSiegeGiantDefenderNonDeath"/>
            <SoundLink value="Merc_SiegeTroll_Vox_Kneel"/>
            <VoiceLink value=""/>
        </DeathArray>
        <DeathArray index="Blast" AnimProps="Unpowered" ModelLink="MercSiegeGiantDefenderNonDeath"/>
